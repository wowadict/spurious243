Namespace PacketLogger

    Public Module OpcodeHandlers
        Public RSOpcodeNames As New Hashtable
        Public PacketHandlers As New Hashtable
        Public PacketHandlerScripts As New ArrayList
        Delegate Sub HandlePacket(ByRef Packet As PacketClass)

        Public Sub AddPacketHandler(ByVal Source As String, ByVal Opcode As Integer)
            Dim tmpScript As New ScriptedObject("scripts\" & Source, "", True)
            Dim index As Integer = PacketHandlerScripts.Add(tmpScript.Invoke("ScriptedPacketHandler"))
            tmpScript.Dispose()

            PacketHandlers(Opcode) = CType(AddressOf CType(PacketHandlerScripts(index), BasePacketHandler).OnRecv, HandlePacket)
        End Sub

        Public Sub InitializeOpcodes()
            RSOpcodeNames(CType(0, Integer)) = "CMD_AUTH_LOGON_CHALLENGE"
            RSOpcodeNames(CType(1, Integer)) = "CMD_AUTH_LOGON_PROOF"
            RSOpcodeNames(CType(2, Integer)) = "CMD_AUTH_RECONNECT_CHALLENGE"
            RSOpcodeNames(CType(3, Integer)) = "CMD_AUTH_RECONNECT_PROOF"
            RSOpcodeNames(CType(&H10, Integer)) = "CMD_REALM_LIST"
            RSOpcodeNames(CType(&H30, Integer)) = "XFER_INITIATE"
            RSOpcodeNames(CType(&H31, Integer)) = "XFER_DATA"
            RSOpcodeNames(CType(&H32, Integer)) = "XFER_ACCEPT"
            RSOpcodeNames(CType(&H33, Integer)) = "XFER_RESUME"
            RSOpcodeNames(CType(&H34, Integer)) = "XFER_CANCEL"
        End Sub
    End Module

